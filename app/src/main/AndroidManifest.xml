<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.scu.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Golden Coin"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".WelcomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignIn"
            android:exported="true"
            android:label="Sign In"
            android:parentActivityName=".WelcomePage">

        </activity>
        <activity
            android:name=".ErrorDialog"
            android:exported="true">

        </activity>
        <activity
            android:name=".SecuritySetting"
            android:exported="true"
            android:label="Security Setting">

        </activity>
        <activity
            android:name=".RequireAuthentication"
            android:exported="true"
            android:label="Require Authentication"
            android:parentActivityName=".SecuritySetting">

        </activity>

        <activity android:name=".HistoryI"
            android:exported="true"
            android:label="History"/>

        <activity android:name=".ChangeDefaultCard"
            android:exported="true"
            android:label="Default Card"/>

    </application>

</manifest>